# ===== API CONFIGURATION =====
REACT_APP_API_BASE_URL=http://localhost:8000/api
REACT_APP_WS_BASE_URL=ws://localhost:8000/ws

# ===== APPLICATION SETTINGS =====
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=1.0.0
REACT_APP_NAME="GEB - Dynamic Pricing & BI System"

# ===== FEATURES FLAGS =====
REACT_APP_ENABLE_AI_INSIGHTS=true
REACT_APP_ENABLE_ADVANCED_ANALYTICS=true
REACT_APP_ENABLE_REAL_TIME_UPDATES=true
REACT_APP_ENABLE_EXPORT_FEATURES=true

# ===== CHARTS & VISUALIZATION =====
REACT_APP_CHART_THEME=light
REACT_APP_DEFAULT_CHART_HEIGHT=400
REACT_APP_ENABLE_3D_CHARTS=false

# ===== PERFORMANCE =====
REACT_APP_CACHE_TIMEOUT=300000
REACT_APP_API_TIMEOUT=30000
REACT_APP_REFRESH_INTERVAL=60000

# ===== EXTERNAL SERVICES =====
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_HOTJAR_ID=your-hotjar-id
REACT_APP_SENTRY_DSN=your-sentry-dsn

# ===== AUTHENTICATION =====
REACT_APP_JWT_EXPIRY=3600
REACT_APP_REFRESH_TOKEN_EXPIRY=86400

# ===== UI/UX =====
REACT_APP_DEFAULT_LANGUAGE=es
REACT_APP_DEFAULT_CURRENCY=PEN
REACT_APP_TIMEZONE=America/Lima

# ===== DEBUG =====
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info